{
  "name": "gh-pages",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "clearCache": "rm -rf node_modules/.cache",
    "deploy": "next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "next": "latest",
    "next-redux-wrapper": "^1.3.5",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-define": "^1.3.0",
    "eslint": "^4.19.1",
    "eslint-config-react-native": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "redux-logger": "^3.0.6"
  },
  "eslintConfig": {
    "extends": "react-native",
    "rules": {
      "react/jsx-filename-extension": 0,
      "import/prefer-default-export": 0,
      "jsx-a11y/anchor-is-valid": 0
    }
  },
  "eslintIgnore": [
    "*.test.js",
    "*.spec.js"
  ]
}
